<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="PHP">
        <added pattern="\Illuminate\Support\Facades\Route.group(attributes,routes)" />
        <added pattern="\Illuminate\Database\Eloquent\Concerns\QueriesRelationships.withCount(relations)" />
        <added pattern="\Illuminate\Support\Facades\Route.post(uri,action)" />
        <added pattern="\Illuminate\Database\Eloquent\Builder.where(column,operator,value,boolean)" />
      </blacklist>
    </blacklists>
    <option id="php.show.names.for.all.args" value="true" />
    <disabledLanguages>
      <language id="TypeScript" />
      <language id="Angular2Html" />
      <language id="JavaScript" />
      <language id="Vue" />
      <language id="SQL" />
    </disabledLanguages>
  </component>
</application>